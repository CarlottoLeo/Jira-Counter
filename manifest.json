{
    "name": "Jira Counter",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "author": "Leonardo Dias Carlotto e Matheus Menegat",
    "content_scripts": [
        {
            "matches": ["<all_urls>"],
            "js": ["index.js"]
        }
    ],
    "browser_action": {
        "default_popup": "index.html",
        "default_title": "Jira Counter",
        "default_icon": "icon.png"
    }
}
